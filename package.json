    "vitest": "^2.1.5",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.9"
  },
  "volta": {
    "node": "20.17.0",
    "npm": "10.8.3"
  },
  "packageManager": "npm@10.8.3",
